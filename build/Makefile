run:
	@g++ ../testing/test.cpp ../src/dominio/arbol.cpp ../src/dominio/utilidades.cpp ../src/presentacion/interfaz.cpp -o test
	@./test.exe
	@erase test.exe

com:
	@g++ ../testing/test.cpp ../src/dominio/arbol.cpp ../src/dominio/utilidades.cpp ../src/presentacion/interfaz.cpp -o test
	@./test

main:
	@g++ ../src/main.cpp ../src/dominio/arbol.cpp ../src/dominio/utilidades.cpp ../src/presentacion/interfaz.cpp -o main
	@./main
	@rm main

